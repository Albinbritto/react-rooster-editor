{
  "name": "react-rooster-editor",
  "version": "0.1.2",
  "description": "A lightweight React wrapper around RoosterJS for building rich text editors",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && npm run copy:css",
    "copy:css": "node scripts/copy-css.cjs",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "roosterjs",
    "rich-text-editor",
    "wysiwyg",
    "editor",
    "typescript"
  ],
  "author": "Albin Britto",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "roosterjs": "^9.40.0",
    "roosterjs-color-utils": "^1.1.0",
    "roosterjs-content-model-api": "^9.40.0",
    "roosterjs-content-model-core": "^9.40.0",
    "roosterjs-content-model-dom": "^9.40.0",
    "roosterjs-content-model-plugins": "^9.40.0",
    "roosterjs-content-model-types": "^9.40.0",
    "@floating-ui/react": "^0.27.16",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/file-saver": "^2.0.7",
    "docx": "^9.5.1",
    "docx-preview": "^0.3.7",
    "file-saver": "^2.0.5",
    "html-docx-js-typescript": "^0.1.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "react-icons": "^5.5.0",
    "react-if": "^4.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
